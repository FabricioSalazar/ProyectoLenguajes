	<queryString language="SQL">
		<![CDATA[SELECT producto.nombre, categoria.nombreCategoria, SUM(productosdetallesorden.cantidad) as Total FROM orden left join detalleorden on orden.idOrden = detalleorden.idOrden
 left join productosdetallesorden on detalleorden.idDetalleOrden = productosdetallesorden.idDetalleOrden
 left join producto on productosdetallesorden.idProducto = producto.idProducto
 left join categoria_producto on producto.idProducto = categoria_producto.idProducto 
 left join categoria on categoria_producto.idCategoria = categoria.idCategoria
 where MONTH(fechaCompra) = MONTH(now()) order by Total;]]>
	</queryString>
